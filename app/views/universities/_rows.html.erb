<% @universities_page.each do |university| %>
    <tr>
        <td class="text-muted"><%= university.id %></td>
        <td><%= university.name %></td>
        <td>
            <%= link_to 'Edit', edit_university_path(university),class: 'btn btn-warning' %>
        </td>
        <td>
            <%= link_to 'Del', university_path(university), class: 'btn btn-danger',
                        method: :delete,
                        data: { confirm: 'Вы уверны? Запись будет удалена!' } %>
        </td>
    </tr>
<% end %>