<div class="page-header">
  <h1>Шаблоны приказов</h1>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th class="text-muted">#</th>
      <th>Название</th>
      <th></th>
    </tr>
  </thead>
  <% @order_templates.each do |template| %>
    <tr>
      <td class="text-muted"><%= template.id %></td>
      <td><%= template.name %></td>
      <td>
        <%= link_to edit_office_order_template_path(template) do %>
          <span class="glyphicon glyphicon-edit"></span> Редактировать
        <% end %>
      </td>
    </tr>
  <% end %>
</table>