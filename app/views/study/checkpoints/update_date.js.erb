$('.change_date_buttons[data-value="<%= @checkpoint.id %>"]').parents('tr').find('.date').html("<%= l @checkpoint.date %>");
$('#not-full-classes').html("<%= escape_javascript (render(partial: 'not_full',
locals: { classes: @discipline.classes, discipline: @discipline })).html_safe %>");
$('#classes_rows').html("<%= escape_javascript (render(partial: 'classes',
locals: { discipline: @discipline })).html_safe %>");
$('#changeDate').modal('hide');
var root = $('#matrixHQ').attr('href');
$('.change_date_buttons').click(function(){
  $.ajax({
    url: root+'study/disciplines/'+$(this).data('parent')+'/checkpoints/'+$(this).data('value')+'/change_date'
  })
})
