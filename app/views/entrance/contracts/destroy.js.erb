if ($('.entrant_contract_buttons').length > 0){
  window.location.reload();
}
$('.modal-backdrop').remove();
$('body').on('hidden.bs.modal', '.modal', function () {
  $(this).removeData('bs.modal');
});
$(document.getElementById('contract_for_<%= @application.id %>')).html(
    '<%=j raw(render partial: 'entrance/contracts/contract_button', locals: {a: @application, from_entrant: false}) %>'
);
$(document.getElementById('contract_for_<%= @application.id %>')).find('.contract-btn').addClass('btn-default').removeClass('btn-success');